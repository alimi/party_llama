<p>Douglass-Myers</p>

<%= form_with scope: :douglass_myers_response, url: douglass_myers_responses_path do |form| %>
  <%= form.collection_check_boxes :attending_guest_ids, @guests, :id, :name do |b| %>
    <%= b.check_box checked: b.object.attending_douglass_myers?  %>
    <%= b.label %>
  <% end %>

  <%= form.submit "Next" %>
<% end %>
